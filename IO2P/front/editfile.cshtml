@{
  Layout="front/mainWrapper.cshtml";
}
<div class="content">
    <h1>Edit</h1>

    <form action="" enctype="multipart/form-data" method="POST">

        <div class="form-group">
           <label for="name-sel">Nazwa pliku:</label><br>
           <input id="name-sel" name="filename" />
        </div>
        <div class="form-group">
           <label for="cat-sel">Kategoria:</label><br>
           <input id="cat-sel" name="category" />
        </div>
         <input class="btn btn-success" type="submit" value="Zapisz" />
    </form>

    <script>
       $(document).ready(function () {
           $('#name-sel').val($.url().param('filename'));
           $('#cat-sel').val($.url().param('category'));
       });
    </script>
</div>
